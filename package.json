{
  "name": "typescript-library-starter",
  "version": "0.0.0",
  "description": "Minimal starter repository for a publishable Typescript library",
  "repository": {
    "type": "git",
    "url": "git@github.com:abhishekbhardwaj/typescript-library-starter.git"
  },
  "author": "Abhishek Bhardwaj <abhishek66ster@gmail.com>",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    "types": "./dist/esm/index.d.ts",
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "default": "./dist/esm/index.js"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "commit": "cz",
    "console": "tsx ./src/console.ts",
    "dev:open-bundle-visualizer": "open rollup-plugin-visualizer-stats.html",
    "test": "NODE_ENV=test vitest run",
    "test:watch": "NODE_ENV=test vitest",
    "test:coverage": "NODE_ENV=test vitest run --coverage",
    "build": "yarpm clean && rollup -c --bundleConfigAsCjs",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "publish-lib": "yarpm run build && yarpm publish",
    "publish-beta": "yarpm run build && yarpm publish --tag beta",
    "publish-dryrun": "yarpm run build && yarpm publish --dry-run",
    "clean": "rimraf dist rollup-plugin-visualizer-stats.html",
    "reset": "yarpm clean && git clean -xdf node_modules",
    "update-all-dependencies": "pnpm dlx npm-check-updates -u",
    "lint": "eslint .",
    "format": "yarpm run lint --fix",
    "prepare": "is-ci || husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@size-limit/preset-big-lib": "^11.1.6",
    "@types/node": "^22.13.1",
    "commander": "^13.1.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "devmoji": "^2.3.0",
    "eslint": "^9",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^15.4.3",
    "open": "^10.1.0",
    "prettier": "^3.5.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.6",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "semantic-release": "^24.2.2",
    "size-limit": "^11.1.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5",
    "yarpm": "^1.2.0"
  },
  "size-limit": [
    {
      "path": "dist/*.js"
    }
  ],
  "packageManager": "pnpm@10.3.0"
}
